{
  "id": null,
  "title": "Model Server Dashboard",
  "uid": "model-server",
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "type": "timeseries",
      "title": "p95 Latency (ms)",
      "id": 1,
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(model_prediction_latency_seconds_bucket[5m])) by (le, model)) * 1000",
          "legendFormat": "{{model}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "ms" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Throughput (req/s)",
      "id": 2,
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "sum by (model) (rate(model_predictions_total[1m]))",
          "legendFormat": "{{model}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "req/s" },
        "overrides": []
      }
    },
    {
      "type": "timeseries",
      "title": "Error Rate (%)",
      "id": 3,
      "datasource": { "type": "prometheus", "uid": "prometheus" },
      "targets": [
        {
          "expr": "100 * (sum by (model) (increase(model_prediction_errors_total[5m])) / sum by (model) (increase(model_predictions_total[5m])))",
          "legendFormat": "{{model}}"
        }
      ],
      "fieldConfig": {
        "defaults": { "unit": "percent" },
        "overrides": []
      }
    }
  ]
}
